[Resolve]
{% if forwarding %}
DNS=127.0.0.1
FallbackDNS=127.0.0.1
{% else %}
DNS=fd5d:2ddd:f4a5::1
FallbackDNS=172.16.33.1
{% endif %}
Domains=~.
DNSSEC=no
DNSOverTLS=no
MulticastDNS=no
LLMNR=no
Cache=yes
CacheFromLocalhost=yes
DNSStubListener=yes
{% if forwarding %}
DNSStubListenerExtra=fd5d:2ddd:f4a5::1
DNSStubListenerExtra=172.16.33.1
{% endif %}
ReadEtcHosts=no
ResolveUnicastSingleLabel=no
