[Match]
Name=wan

[Network]
DHCP=yes
LLMNR=no
MulticastDNS=no
Domains=~push.apple.com ~push-apple.com.akadns.net ~courier-push.apple.com ~courier-push-apple.com.akadns.net
DNS=192.168.1.254
DNSSEC=allow-downgrade
